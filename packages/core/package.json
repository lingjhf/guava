{
  "name": "@lingjhf/guava",
  "version": "0.0.4",
  "description": "UI Components",
  "author": "lingjhf",
  "license": "MIT",
  "homepage": "https://lingjhf.github.io/guava/",
  "repository": {
    "type": "git",
    "url": "https://github.com/lingjhf/guava"
  },
  "type": "module",
  "scripts": {
    "core:build": "vite build && vite build -c ./vite.config.umd.ts && rollup -c ",
    "lint:fix": "eslint --fix ."
  },
  "files": [
    "lib",
    "umd",
    "README.md",
    "package.json"
  ],
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.js"
    },
    "./lib/*": {
      "types": "./lib/*.d.ts",
      "import": "./lib/*.js"
    }
  },
  "keywords": [
    "ui",
    "web component",
    "solid",
    "solidjs",
    "components"
  ],
  "dependencies": {
    "@lingjhf/guava-theme": "workspace:^",
    "@types/color": "^3.0.3",
    "color": "^4.2.3",
    "dayjs": "^1.11.7",
    "highlight.js": "^11.8.0",
    "mitt": "^3.0.1",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@solidjs/testing-library": "^0.8.1",
    "@testing-library/jest-dom": "^5.16.5",
    "autoprefixer": "^10.4.14",
    "jsdom": "^22.1.0",
    "vite-plugin-solid": "^2.7.0",
    "vitest": "^0.32.2"
  }
}
